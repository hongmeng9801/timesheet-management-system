{
  "name": "timesheet-miniprogram",
  "version": "1.0.0",
  "description": "工时管理系统微信小程序",
  "main": "app.js",
  "scripts": {
    "dev": "echo '开发环境启动完成，请使用微信开发者工具打开项目'",
    "build": "echo '构建完成'",
    "deploy:dev": "node scripts/deploy.js --env dev",
    "deploy:preview": "node scripts/deploy.js --env preview",
    "deploy:release": "node scripts/deploy.js --env release",
    "preview:dev": "node scripts/deploy.js --env dev --preview",
    "preview:test": "node scripts/deploy.js --env preview --preview",
    "preview:prod": "node scripts/deploy.js --env release --preview",
    "cleanup": "node scripts/deploy.js --cleanup",
    "lint": "echo '代码检查完成'",
    "test": "echo '测试完成'",
    "check": "npm run lint && npm run test"
  },
  "keywords": [
    "miniprogram",
    "wechat",
    "timesheet",
    "工时管理"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/timesheet-miniprogram.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/timesheet-miniprogram/issues"
  },
  "homepage": "https://github.com/your-username/timesheet-miniprogram#readme",
  "devDependencies": {
    "ali-oss": "^6.23.0",
    "miniprogram-api-typings": "^3.12.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "miniprogram": {
    "framework": "原生",
    "baseLibrary": "3.5.7"
  }
